<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:background="#5BCCD3"

        tools:ignore="MissingConstraints" >

        <ImageView
            android:id="@+id/imgsong"
            android:layout_width="58dp"
            android:layout_height="42dp"
            android:src="@drawable/img_3"
            tools:ignore="MissingConstraints" />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="425dp"
        android:layout_height="40dp"


        android:background="#5BCCD3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="696dp">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"


            android:layout_alignParentBottom="true"
            android:layout_marginBottom="25dp"
            android:text="Copyright Â© 2020 Didactic. Todos los derechos reservados."
            android:textSize="10dp"
            tools:ignore="TextSizeCheck" />

        <ImageView
            android:id="@+id/imgface"
            android:layout_width="31dp"
            android:layout_height="33dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="104dp"
            android:src="@drawable/img"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imginsta"
            android:layout_width="31dp"
            android:layout_height="33dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="61dp"
            android:src="@drawable/img_1"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imgtiktok"
            android:layout_width="31dp"
            android:layout_height="33dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="22dp"
            android:src="@drawable/img_2"
            tools:ignore="MissingConstraints" />


    </RelativeLayout>

    <TextView
        android:id="@+id/textname"
        android:layout_width="261dp"
        android:layout_height="45dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="72dp"
        android:text="Name"

        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.041" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="46dp"
        android:background="@drawable/translucid"
        android:scaleType="fitCenter"
        android:src="@drawable/img_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textname"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck"
        tools:layout_editor_absoluteY="95dp"></ImageButton>

    <ImageView
        android:id="@+id/my_image_view"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="@drawable/circle"
        android:src="@drawable/img_5"
        app:layout_constraintEnd_toStartOf="@+id/textname"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="92dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="351dp"
        android:layout_height="263dp"
        android:src="@drawable/img_6"
        app:flow_verticalAlign="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="234dp"
        tools:ignore="MissingConstraints"></ImageView>


</androidx.constraintlayout.widget.ConstraintLayout>